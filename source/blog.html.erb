---
pageable: true
per_page: 10
---

<%= partial "partials/navbar" %>

<div class="container-fluid">
	<% page_articles.each_with_index do |article, i| %>
	  <h4>
	  	<a href="<%=config[:host_prefix]%><%= article.url %>"><%=article.title %></a>
	  	<span><%= article.date.strftime('%b %e') %></span>
	  </h4>
	<% end %>
</div>
