---
pageable: true
per_page: 10
---

<%= partial "partials/navbar" %>

<div class="container-fluid">
	<% page_articles.each_with_index do |article, i| %>
	<% article_path = config[:host_prefix] + "/" + article.path.to_s %>
	  <h4><%= link_to article.title, article_path %> <span><%= article.date.strftime('%b %e') %></span></h4>
	<% end %>
</div>
